{
  "name": "phoenix-tests",
  "description": "Simple Travis-CI check for JSHint (Code Linting)",
  "author": "edward.moyse@cern.ch",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "ng build --prod --output-path docs --base-href \"/phoenix/\" && cp ./docs/index.html ./docs/404.html",
    "test:coverage": "rm -rf ./coverage && ng test --no-watch --code-coverage",
    "coveralls": "cat ./coverage/phoenix/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0-next.0",
    "@angular/cdk": "^8.1.2",
    "@angular/common": "~9.0.0-next.0",
    "@angular/compiler": "~9.0.0-next.0",
    "@angular/core": "~9.0.0-next.0",
    "@angular/forms": "~9.0.0-next.0",
    "@angular/platform-browser": "~9.0.0-next.0",
    "@angular/platform-browser-dynamic": "~9.0.0-next.0",
    "@angular/router": "~9.0.0-next.0",
    "@tweenjs/tween.js": "^17.4.0",
    "@types/three": "^0.103.2",
    "core-js": "^2.5.4",
    "coveralls": "^3.0.4",
    "dat.gui": "^0.7.6",
    "rxjs": "~6.5.2",
    "stats-js": "^1.0.1",
    "three": "^0.104.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.25",
    "@angular/cli": "~8.2.0",
    "@angular/compiler-cli": "~9.0.0-next.0",
    "@angular/language-service": "~9.0.0-next.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3"
  }
}
